#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    int T;
    cin >>T;
    while(T--){
        int n,k,a[100000];
        cin >> n>> k;
        int res=0;
        for(int i=0;i<n;i++){
            cin >> a[i];
            if(a[i]==k){
                res=1;
            }
        }
        if(res==1){
            cout <<"1"<<endl;
        }else{
            cout <<"-1"<<endl;
        }
    }
}
