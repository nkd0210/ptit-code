#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
    int T;
    cin >> T;
    while(T--){
        int m,n,k;
        cin >> m >> n >> k;
        int a[100000];
        int b[100000];
        vector<int>v;
        for(int i=0;i<m;i++){
            cin >> a[i];
            v.push_back(a[i]);
        }
        for(int i=0;i<n;i++){
            cin >> b[i];
            v.push_back(b[i]);
        }
        sort(v.begin(), v.end());
        cout << v[k-1] << endl;
    }
}
