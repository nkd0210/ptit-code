#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
	int T;
	cin >>T;
	while(T--){
		int n,m;
		ll a[100000];
		ll b[100000];
		cin >> n >> m;
		for(int i=0;i<n;i++){
			cin >> a[i];
		}
		for(int j=0;j<m;j++){
			cin >> b[j];
		}
		sort(a,a+n);
		sort(b,b+m);
		cout << a[n-1] * b[0] << endl;
	}
}
